{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "run-s build:*",
    "build:css": "tailwindcss --output ./app/styles/tailwind.css --minify --postcss",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:css": "tailwindcss --output ./app/styles/tailwind.css --watch --postcss",
    "dev:remix": "remix dev",
    "codegen": "graphql-codegen --require dotenv/config --config codegen.yml",
    "clean": "del .cache .netlify app/graphql/index.ts app/styles/tailwind.css public/build netlify/functions/server/build",
    "lint": "eslint --cache --fix --ignore-path .gitignore .",
    "format": "prettier --write --ignore-path .gitignore ."
  },
  "dependencies": {
    "@headlessui/react": "^1.7.18",
    "@heroicons/react": "^2.1.1",
    "@netlify/functions": "^2.4.1",
    "@remix-run/netlify": "1.19.3",
    "@remix-run/node": "1.19.3",
    "@remix-run/react": "1.19.3",
    "@urql/core": "^4.2.2",
    "date-fns": "^3.2.0",
    "gql.tada": "1.0.2-canary-ddc98eea4e4c6cc9bec5b55a03ab1025f146523e",
    "graphql": "^16.8.1",
    "isbot": "^4.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "type-fest": "^4.9.0"
  },
  "devDependencies": {
    "@0no-co/graphql.web": "^1.0.4",
    "@0no-co/graphqlsp": "^1.0.3",
    "@remix-run/dev": "1.19.3",
    "@remix-run/eslint-config": "1.19.3",
    "@remix-run/serve": "1.19.3",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "del-cli": "^5.1.0",
    "eslint": "^8.56.0",
    "eslint-plugin-prefer-let": "^3.0.1",
    "eslint-plugin-remix-react-routes": "^1.0.5",
    "netlify-cli": "^17.14.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.33",
    "postcss-100vh-fix": "^1.0.2",
    "prettier": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18"
  },
  "sideEffects": false
}
