{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "npm run codegen && run-s build:*",
    "build:css": "tailwindcss --output ./app/styles/tailwind.css --minify --postcss",
    "build:remix": "remix build",
    "dev": "npm run codegen && run-p dev:*",
    "dev:css": "tailwindcss --output ./app/styles/tailwind.css --watch --postcss",
    "dev:remix": "remix dev",
    "codegen": "graphql-codegen --require dotenv/config --config codegen.yml",
    "clean": "del .cache .netlify app/graphql/index.ts app/styles/tailwind.css public/build netlify/functions/server/build",
    "lint": "eslint --cache --fix --ignore-path .gitignore .",
    "format": "prettier --write --ignore-path .gitignore ."
  },
  "dependencies": {
    "@headlessui/react": "1.7.7",
    "@heroicons/react": "2.0.13",
    "@mcansh/fetcher": "0.2.2",
    "@netlify/functions": "1.4.0",
    "@remix-run/netlify": "1.10.0",
    "@remix-run/node": "1.10.0",
    "@remix-run/react": "1.10.0",
    "date-fns": "2.29.3",
    "graphql": "16.6.0",
    "graphql-request": "5.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "type-fest": "3.5.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.16.4",
    "@graphql-codegen/typescript": "2.8.7",
    "@graphql-codegen/typescript-graphql-request": "4.5.8",
    "@graphql-codegen/typescript-operations": "2.5.12",
    "@remix-run/dev": "1.10.0",
    "@remix-run/eslint-config": "1.10.0",
    "@remix-run/serve": "1.10.0",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/forms": "0.5.3",
    "@tailwindcss/typography": "0.5.9",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "autoprefixer": "10.4.13",
    "del-cli": "5.0.0",
    "eslint": "8.31.0",
    "eslint-plugin-prefer-let": "3.0.1",
    "eslint-plugin-remix-react-routes": "1.0.5",
    "netlify-cli": "12.7.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.31",
    "postcss-100vh-fix": "1.0.2",
    "prettier": "2.8.2",
    "prettier-plugin-tailwindcss": "0.2.1",
    "tailwindcss": "3.2.4",
    "typescript": "4.9.4"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
